{
  "name": "paynterperformance",
  "description": "Kai Paynter's website",
  "version": "1.0.0",
  "author": "Boipelo Mawasha",
  "dependencies": {
    "gatsby": "^5.13.6",
    "gatsby-plugin-google-gtag": "^5.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-offline": "^6.13.2",
    "gatsby-plugin-react-helmet": "^6.13.1",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sass": "^6.13.1",
    "gatsby-plugin-sitemap": "^6.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "gh-pages": "^6.1.1",
    "react": "^19.0.0",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.77.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "setup": "npm install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "branchCleanup": "git branch -d gh-pages",
    "branchSetup": "git checkout -b gh-pages",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "reset": "git checkout main",
    "deploy:full": "npm run branchSetup && npm run deploy && npm run reset && npm run branchCleanup"
  },
  "devDependencies": {
    "prettier": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaipaynter/paynterperformance"
  },
  "homepage": "https://paynterperformance.com"
}
